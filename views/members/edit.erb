<h2>Edit <%= @member.format_name %></h2>
<form method="POST" action="/members/<%= @member.id %>">
  <div class="form-wrapper">

    <div class="form-el">
      <label for="first_name">First Name:</label>
      <input type="text" name="first_name" id="first_name" value="<%= @member.first_name %>"/>
    </div>

    <div class="form-el">
      <label for="last_name">Last Name:</label>
      <input type="text" name="last_name" id="last_name" value="<%= @member.last_name%>"/>
    </div>

    <div class="form-el">
      <label for="gender">gender:</label>
      <input type="text" name="gender" id="gender" value="<%= @member.gender%>"/>
    </div>

    <div class="form-el">
      <label for="age">Age:</label>
      <input type="number" name="age" id="age" value="<%= @member.age %>"/>
    </div>

    <div class="form-el">
      <input type="submit" value="Edit Member"/>
    </div>

  </div>
</form>
