<h2>Sessions</h2>

<div class="session">
  <h4><a href="/sessions/new"> Add session</a></h4>
</div>

<div id="session-wrapper">
  <% @sessions.each do |session| %>

  <div class="session">
    <h3><a href="/sessions/<%= session.id %>"><%= session.format_name %></a></h3>
    <p>type: <%= session.type %></p>
    <p>start time: <%= session.start_time %></p>
  </div>

  <!-- <div class="session">
    <h4><a href="/sessions/<%= session.id%>/edit"> Edit session</a></h4>
  </div> -->

  <% end %>
</div>
